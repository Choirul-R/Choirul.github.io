{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-undef":"off",
        "no-unused-vars":["warn", {"vars":"all", "args":"after-used", "ignoreRestSiblings":false}],
        "no-prototype-builtins":"warn"
    }
}